{
    "name": "thelhc/geo-ip",
    "description": "Get user geography from ip address",
    "type": "composer-plugin",
    "authors": [
        {
            "name": "Aaron Kaczmarek",
            "email": "aaronmichaelmusic@gmail.com"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "TheLHC\\GeoIP\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheLHC\\GeoIP\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
